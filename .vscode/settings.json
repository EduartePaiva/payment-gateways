{
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[json]": {
		"editor.quickSuggestions": {
			"strings": true
		},
		"editor.suggest.insertMode": "replace",
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[css]": {
		"editor.suggest.insertMode": "replace",
		"cSpell.fixSpellingWithRenameProvider": false,
		"editor.defaultFormatter": "biomejs.biome"
	},
	"cSpell.words": ["bson", "templ"]
}
